y = int(input("Enter a year: "))
x = y-2000
c = x//4
m = x - c
n = m*2
v = n-c
j = v%7
print(j)
match j:
    case 1:
        print("Monday")
    case 2: 
        print("Tuesday")
    case 3:
        print("Wednesday")
    case 4: 
        print("Thursday")
    case 5:
        print("Friday")
    case 6: 
        print("Saturday")
    case 0: 
        print("Sunday")
